Algoritmo intercambio_de_3_vallores 
	
	Escribir "introdusca el valorA"
	Leer valorA
	
	Escribir "introdusca el valorB"
	Leer valorB
	
	Escribir "introdusca el valorC"
	Leer valorC
	
	C <- A 
	B <- C
	A <- B
	
	Escribir "EL nuevo valor de A es: ", valorC
	Escribir "El nuevo valor de B es: ", valorA
	Escribir "El nuevo valor de C es: ", valorB
	
FinAlgoritmo
