Algoritmo problema_matematico 
	
	Definir valor1 Como Real
	Definir Valor2 Como Real
	Definir  Respuesta Como Real
	Definir RespuestaDevalor2 Como Real
	
	Escribir "Ingrese el valor1"
	Leer valor1
	
	Escribir "Ingrese el valor2"
	Leer valor2
	
	RespuestaDevalor2 <- valor1/100 * valor2
	Respuesta <- valor2 - RespuestaDevalor2
	
	Escribir "La cantidad que me darias es: ", RespuestaDevalor2
	Escribir "La cantidad con la que te quedas es: ", Respuesta
	
FinAlgoritmo
